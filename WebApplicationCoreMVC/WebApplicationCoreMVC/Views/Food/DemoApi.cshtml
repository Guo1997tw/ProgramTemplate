<table class="table">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">ProductID</th>
            <th scope="col">Last</th>
            <th scope="col">Handle</th>
        </tr>
    </thead>
    <tbody>
        @{
            var temp = ViewBag.Data as List<ProductsViewModel>;
            var i = 1;

            foreach (var r in temp)
            {
                <tr>
                    <th scope="row">@i</th>
                    <td>@r.ProductID</td>
                    <td>@r.ProductName</td>
                    <td>@r.UnitPrice</td>
                </tr>

                i++;
            }
        }
    </tbody>
</table>